.header
  position: fixed;
  z-index: 1;
  top: 0;
  width: 100%;
  padding: 5px 75px;
  background: #f6f6f6;
  box-shadow: 0 0 0 1px rgba(0,0,0,.1), 0 10px 20px -5px rgba(0,0,0,.4);
  opacity: .98;

  &__class
    position relative
    top 25px
    right 70px
    float right
    font-size 40px

  &__image
    width: 150px;
    height: 100px;
